#!/usr/bin/env coffee

{ resolve } = require 'path'

camelize = require resolve __dirname, '..', 'node_modules', 'camelize'

minimist = require resolve __dirname, '..', 'node_modules', 'minimist'

passwd = require resolve __dirname, '..', 'password-generator'

args = minimist process.argv.slice 2

Object.keys(args).map (key) ->

  if key.indexOf("-") > 0

    args[camelize key] = args[key]

    delete args[key]

console.log passwd args
